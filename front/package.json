  "scripts": {
    "postinstall": "patch-package",
    "start": "npx react-native start",
    "test": "jest",
    "lint": "eslint .",
    "clean": "cd android && ./gradlew clean && cd .. && xattr -w com.apple.xcode.CreatedByBuildSystem true ios/build && cd ios && xcodebuild clean && cd ..",
    "cleanAndroid": "cd android && ./gradlew clean && cd ..",
    "android": "react-native run-android",
    "build:android": "./build-android.sh",
    "restartGradle": "cd android && pkill -f '.*GradleDaemon.*' && pkill -f '.*java.*' && ./gradlew --stop && cd ..",
    "copyContent": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --assets-dest ios --dev=false --platform='ios'",
    "clean:ios": "xattr -w com.apple.xcode.CreatedByBuildSystem true ios/build && cd ios && xcodebuild clean && cd ..",
    "run:ios": "react-native run-ios --simulator='iPhone 15 Pro Max'",
    "pod:install": "npx pod-install ios"
  },
